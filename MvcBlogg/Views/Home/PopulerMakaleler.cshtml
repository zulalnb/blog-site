@model IEnumerable<MvcBlogg.Models.Makale>
@{
    Layout = null;
}
<h5 class="card-header">En Çok Okunan Makaleler</h5>
<div class="card-body">
    <div class="col-lg-12">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.MakaleId">@item.Baslik (@item.Okunma) </a></li>
            }
        </ul>
    </div>
</div>